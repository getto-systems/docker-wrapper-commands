#!/bin/bash
. docker-wrapper.sh
docker_wrapper_server nginx "$@"
if [ "$docker_wrapper_server_cmd" == start ]; then
  docker run \
    -d \
    -w $APP_ROOT \
    $(docker_wrapper_server_name) \
    $(docker_wrapper_volumes) \
    "${docker_wrapper_envs[@]}" \
    $(docker_wrapper_image nginx) \
    nginx -g "daemon off;" -c "$DOCKER_WRAPPER_NGINX_CONF_PATH" \
  ;
fi
